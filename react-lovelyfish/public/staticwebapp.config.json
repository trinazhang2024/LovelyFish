{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*"]
  },
  "routes": [
    {
      "route": "/404",
      "serve": "/404.html",
      "statusCode": 404
    }
  ],
  "responseOverrides": {
    "/*": {
      "headers": {
        "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
      }
    }
  }
}